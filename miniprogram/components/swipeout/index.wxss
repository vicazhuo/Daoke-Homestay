.i-swipeout-wrap {
    background: #fff;
    position: relative;
    overflow: hidden;
}

.i-swipeout-item {
    width: 100%;
    box-sizing: border-box;
    transition: transform 0.2s ease;
    transition: transform 0.2s ease,-webkit-transform 0.2s ease;
    font-size: 28rpx;
}

.i-swipeout-content {
    white-space: nowrap;
    overflow: hidden;
}

.i-swipeout-button-right-group {
    position: absolute;
    right: -100%;
    top: 0;
    height: 100%;
    z-index: 1;
    width: 100%;
}

.i-swipeout-button-right-item {
    height: 100%;
    float: left;
    white-space: nowrap;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
}